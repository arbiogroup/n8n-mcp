{
  "family": "arbio-n8n-mcp",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::785747132622:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "cpu": 0,
      "environment": [],
      "environmentFiles": [
        {
          "name": "GOOGLE_CALLBACK_URL",
          "value": "https://flow.arbio.io/auth/google/callback"
        },{
          "name": "OAUTH_REDIRECT_URI",
          "value": "https://flow.arbio.io/auth/google/callback"
        },
        {
          "name": "JWT_REFRESH_EXPIRES_IN",
          "value": "7d"
        },
        {
          "name": "OAUTH_DOMAIN_RESTRICTION",
          "value": "arbio-group.com"
        },
        {
          "name": "JWT_EXPIRES_IN",
          "value": "1h"
        },
        {
          "name": "OAUTH_REQUIRE_DOMAIN_VERIFICATION",
          "value": "true"
        }
      ],
      "essential": true,
      "image": "785747132622.dkr.ecr.eu-central-1.amazonaws.com/arbio/n8n-mcp:latest",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/arbio-n8n-mcp",
          "awslogs-create-group": "true",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "mountPoints": [],
      "name": "arbio-n8n-mcp",
      "portMappings": [
        {
          "appProtocol": "http",
          "containerPort": 3000,
          "hostPort": 3000,
          "name": "arbio-n8n-mcp-3000-tcp",
          "protocol": "tcp"
        }
      ],
      "secrets": [
        {
          "name": "N8N_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:eu-central-1:785747132622:secret:arbio/flow/N8N_API_KEY-NGLapS"
        },
        {
          "name": "GOOGLE_CLIENT_ID",
          "valueFrom": "arn:aws:secretsmanager:eu-central-1:785747132622:secret:arbio/flow/GOOGLE_CLIENT_ID-RYEZMb"
        },
        {
          "name": "GOOGLE_CLIENT_SECRET",
          "valueFrom": "arn:aws:secretsmanager:eu-central-1:785747132622:secret:arbio/flow/GOOGLE_CLIENT_SECRET-ZxJu5p"
        },
        {
          "name": "JWT_SECRET",
          "valueFrom": "arn:aws:secretsmanager:eu-central-1:785747132622:secret:arbio/flow/JWT_SECRET-rhjnZL"
        },
        {
          "name": "SESSION_SECRET",
          "valueFrom": "arn:aws:secretsmanager:eu-central-1:785747132622:secret:arbio/flow/SESSION_SECRET-CJqDtY"
        }
      ],
      "systemControls": [],
      "ulimits": [],
      "volumesFrom": []
    }
  ]
}
