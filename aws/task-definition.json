          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "mountPoints": [],
      "name": "arbio-n8n-mcp",
      "portMappings": [
        {
          "appProtocol": "http",
          "containerPort": 3000,
          "hostPort": 3000,
          "name": "arbio-n8n-mcp-3000-tcp",
          "protocol": "tcp"
        }
      ],
      "secrets": [
        {
          "name": "AUTH_TOKEN",
          "valueFrom": "arn:aws:secretsmanager:eu-central-1:785747132622:secret:arbio/flow/AUTH_TOKEN-HoSfgx"
